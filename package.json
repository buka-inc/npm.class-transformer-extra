    "prepublishOnly": "npm run build",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "test": "jest"
  },
  "keywords": [
    "class-transformer",
    "class",
    "transformer",
    "transform",
    "extra",
    "external"
  ],
  "author": "Val.istar.Guo <val.istar.guo@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/buka-lnc/npm.class-transformer-extra/#readme",
  "bugs": {
    "url": "https://github.com/buka-lnc/npm.class-transformer-extra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buka-lnc/npm.class-transformer-extra.git"
  },
  "devDependencies": {
    "@buka/eslint-config": "^1.5.0",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@jest/globals": "^29.7.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/node": "^20.9.0",
    "@types/ramda": "^0.29.9",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "class-transformer": "^0.5.1",
    "eslint": "^8.53.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "jest-mock": "^29.7.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-patch": "^3.0.2",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^3.4.6"
  },
  "peerDependencies": {
    "class-transformer": "^0.5.1"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "ramda": "^0.29.1"
